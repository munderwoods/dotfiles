! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #0e1f23, #0e1f23, #1b2b34
rofi.color-normal: argb:0000000, #1b2b34, argb:0000000, #0e1f23, #b5bdca
rofi.color-active: argb:0000000, #1b2b34, argb:0000000, #1b2b34, #b5bdca
rofi.color-urgent: argb:0000000, #e12a36, argb:0000000, #dd2a4b, #b5bdca
rofi.font: Dina 8
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.location:0
rofi.width: 640
rofi.lines: 8
rofi.yoffset: 26
rofi.padding: 32

*font Dina:style:Regular
! special
*.foreground:   #b5bdca
*.background:   #0e1f23
*.cursorColor:  #b5bdca

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #6b8f9a
*.color10:      #6b8f9a

! yellow
*.color3:       #53758a
*.color11:      #f0c674

! blue
*.color4:       #5f819d
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b0585a

! cyan
*.color6:       #6b8f9a
*.color14:      #53758a

! white
*.color7:       #707880
*.color15:      #c5c8c6


! Urxvt Config
! -------------------------------------------------------------------------------

  URxvt.font: xft:Dina-10
  URxvt.scrollBar: false
   URxvt.cursorBlink: true
   URxvt.internalBorder: 30
   URxvt.perl-ext: default,matcher
   URxvt.matcher.button: 1
   URxvt.url-launcher: /usr/bin/chromium

#xterm config
XTerm.vt100.faceName: xft:Dina-10
XTerm.vt100.font: 7x13
xterm*borderWidth:0
